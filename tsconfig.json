{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "downlevelIteration": true,
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@atoms/*": [
        "components/atoms/*"
      ],
      "@molecules/*": [
        "components/molecules/*"
      ],
      "@organisms/*": [
        "components/organisms/*"
      ],
      "@templates/*": [
        "components/templates/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    }
  },
  "include": [
    "next.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
